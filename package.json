  "scripts": {
    "dev": "cross-env ENABLE_SOURCE_MAP='true' vite",
    "build": "tsc && vite build",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --maxWorkers=2",
    "lint": "npm run lint:src && npm run lint:e2e",
    "lint:ci": "npm run lint:src:ci && npm run lint:e2e:ci",
    "lint:src": "npx eslint src --ext ts,tsx",
    "lint:src:ci": "npx eslint src --ext ts,tsx -f json --output-file ./.src.eslint.sonar.json || true",
    "lint:e2e": "npx eslint e2e --ext ts,tsx",
    "lint:e2e:ci": "npx eslint e2e --ext ts,tsx -f json --output-file ./.e2e.eslint.sonar.json || true",
    "lint:fix": "npm run lint:src -- --fix && npm run lint:e2e -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "check": "npm run prettier && npm run lint && npm run check:spell",
    "check:ci": "npm run prettier && npm run lint:ci && npm run check:spell && npm run check:package",
    "check:env:prod": "check-env API_BASE_URL STRIPE_PUB_KEY SENTRY_DSN GROWTHBOOK_CLIENT_KEY",
    "check:env:stage": "check-env API_BASE_URL STRIPE_PUB_KEY GROWTHBOOK_CLIENT_KEY",
    "check:package": "npx npmPkgJsonLint .",
    "check:spell": "cspell \"src/**\"",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install",
    "postinstall": "husky install",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "lint-staged": {
    "*.{yml,yaml,json,css,scss,html,md}": "prettier --write",
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mdx-js/react": "1.6.22",
    "@storybook/addon-actions": "7.6.3",
